From: Mike Hommey <glandium@debian.org>
Date: Thu, 23 Jul 2009 17:31:01 +0200
Subject: Avoid linking with system-installed libmozjs when linking js binary
 to it

---
 js/src/Makefile.in |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/js/src/Makefile.in b/js/src/Makefile.in
index 6f680cc..a000a71 100644
--- a/js/src/Makefile.in
+++ b/js/src/Makefile.in
@@ -51,7 +51,7 @@ LIBRARY_NAME	= mozjs
 GRE_MODULE	= 1
 
 SIMPLE_PROGRAMS	= js$(BIN_SUFFIX)
-LIBS		= $(NSPR_LIBS) -L. $(MOZ_JS_LIBS)
+LIBS		= -L. $(MOZ_JS_LIBS) $(NSPR_LIBS)
 # Make sure the library is built before the js binary
 $(SIMPLE_PROGRAMS): $(DLL_PREFIX)$(LIBRARY_NAME)$(DLL_SUFFIX)
 
